  transactionCharge:
    title: Transaction - Charge
    type: object
    properties:
      uuid:
        type: string
        format: uuid
        description: The UUID to identidy the charge
      customerId:
        type: integer
        description: Id of the customer
      amount:
        type: integer
        description: Charge price in cents
      currency:
        type: string
        minLength: 3
        maxLength: 3
        description: Charge price's currency
      transactionType:
        type: string
        enum:
          - charge
      created:
        type: string
        description: Time when the charge transaction was created
        format: date-time
      customerGroupId:
        type: integer
        description: Id of the group the customer belongs to
      customerGroupName:
        type: string
        description: Name of group the customer belongs to
      organizationId:
        type: integer
        description: Id of the EMP of the customer
      paymentType:
        type: string
        enum:
          - Prepaid
          - Postpaid
          - Postpaid (by card)
        Description: The payment type of the customer
      startTime:
        type: string
        format: date-time
        description: Start time of the charge event
      stopTime:
        type: string
        format: date-time
        description: End time of the charge event
      signedMeterValueBegin:
        type:
          - string
          - 'null'
        description: >-
          Signed meter values at the start of the charge event. Required by the
          German calibration law (Eichrecht).
      signedMeterValueEnd:
        type:
          - string
          - 'null'
        description: >-
          Signed meter values at the end of the charge event. Required by the
          German calibration law (Eichrecht).
      duration:
        type: integer
        description: Duration in minutes
        minimum: 0
      wattHour:
        type: integer
        minimum: 0
        description: Power consumed
      stationUuid:
        type: string
        format: uuid
        description: The UUID to identify the station
      rfid:
        type:
          - string
          - 'null'
        description: RFID of the charge, if any.
      empShare:
        type: integer
        description: EMP share in cents of the charge event
      evseId:
        type: integer
        description: The EVSE id of the charging station
      plugType:
        type: string
        enum:
          - AC
          - DC
        Description: The plug type of the charging station
      cdrId:
        type: integer
        Description: Identifier for the charge event
      operatorPrice:
        type: integer
        description: Operator price in cents of the charge event
      operatorCurrency:
        type: string
        description: Currency of the operator price
      operatorId:
        type: integer
        description: Organization Id of the Operator
      freeCharging:
        type: string
        description: 'Is the charge event a free charge: yes or no'
      empTariffId:
        type:
          - integer
          - 'null'
        description: Id of the customer tariff (only for pricing v2)
      empTariffName:
        type:
          - string
          - 'null'
        description: Name of the customer tariff (only for pricing v2)
      fullEvseId:
        type: string
        description: EVSE of the charging station including roaming identifiers
      cpoOperatorId:
        type: string
        description: The roaming identifier of the CPO
      meterStart:
        type: integer
        description: Meter values at the start of the charge event
      meterStop:
        type: integer
        description: Meter values at the start of the charge event
      notes:
        type: array
        description: Additional information concerning the charge event
      eichrechtType:
        type: string
        enum:
          - None
          - SAFE
          - SAM
          - Innogy
        description: The Eichrecht type the charge
      empKwhNetPrice:
        type:
          - integer
          - 'null'
        description: 'Pricing v2: Net price / kWh of the EMP tariff'
      empKwhGrossPrice:
        type:
          - integer
          - 'null'
        description: 'Pricing v2: Gross price / kWh of the EMP tariff'
      empMinuteNetPrice:
        type:
          - integer
          - 'null'
        description: 'Pricing v2: Net price / minute of the EMP tariff'
      empMinuteGrossPrice:
        type:
          - integer
          - 'null'
        description: 'Pricing v2: Gross price / minute of the EMP tariff'
      empSessionNetPrice:
        type:
          - integer
          - 'null'
        description: 'Pricing v2: Net price / session of the EMP tariff'
      empSessionGrossPrice:
        type:
          - integer
          - 'null'
        description: 'Pricing v2: Gross price / session of the EMP tariff'
      station:
        $ref: '#/definitions/station-compact'
    required:
      - uuid
      - stationUuid
      - customerId
      - startTime
      - stopTime
      - duration
      - wattHour
      - amount
      - currency
      - transactionType
      - created
    example:
      uuid: 5a7de520-59ce-4668-b925-ee88d5312b62
      customerId: 13096134
      amount: -70085727
      currency: EUR
      transactionType: charge
      created: '2020-04-20T15:52:58+03:00'
      customerGroupId: 38774053
      customerGroupName: deserunt
      organizationId: 1401758
      paymentType: Prepaid
      startTime: '2020-04-20T13:52:57+03:00'
      stopTime: '2020-04-20T15:52:58+03:00'
      signedMeterValueBegin: >-
        AP;0;3;AMMZ566VVHO23IDLJSWDPNE6F5DQ5QY32GAIJGYAAAABA232ISABC5GRF4AIAAEEAB7Y6AAJROAQAAAAAAABQGRDEMQZXIJAUKMBVIM4DAAAAAAAAAAAYAAAAALYAAAAA====;ZCZOVWEMRUOLCYI53LNLHXDHZD4VIO3FJ5YVTSIT7GPVNKTM2YN6MPRCZSWDI2HRTS5O6MHNNCWUA===;
      signedMeterValueEnd: >-
        AP;1;3;AMMZ566VVHO23IDLJSWDPNE6F5DQ5QY32Q6OYYDCNQAAAFACRC2I4ADGAIJGYAAAAAQ4UDIUABQ7ORF4AIAAEEAB7Y6ADMCYAQAAAAAAABQGRDEMQZXIJAUKMBVIM4DAAAAAAAAAAAYAAAAAMAAAAAA====;HN5NMNDVWKSTU7Y3I3IPKBKN3D7I52BOR3NPR2RSPRFIWEJPDGD5LYY4XG7CPZJN6ZV5BNVZTXMZE===;
      duration: 121
      wattHour: 1000
      stationUuid: 2088eb3b-04b7-4b7d-89b3-37721a3ee974
      rfid: 0420A14A
      empShare: 70085727
      evseId: 56197546
      plugType: AC
      cdrId: 22039210
      operatorPrice: 30670123
      operatorCurrency: EUR
      operatorId: 78655333
      freeCharging: 'No'
      fullEvseId: FI*001*E56197546
      cpoOperatorId: FI*001
      meterStart: 1000
      meterStop: 2000
      eichrechtType: SAFE
      empKwhNetPrice: null
      empKwhGrossPrice: null
      empMinuteNetPrice: null
      empMinuteGrossPrice: null
      empSessionNetPrice: 56520748
      empSessionGrossPrice: 70085727
      station:
        identity: FI_ChARGE_ME
        name: Hi charger
        location:
          id: 56197546
          address: Hi Street 2
          city: Helsinki
          coordinates:
            lat: 60.66868
            lng: 12.54339
        locationType: Public
  charge:
    title: CpoCharge - Response
    type: object
    properties:
      evseId:
        type: integer
      fullEvseId:
        type: string
      locationId:
        type: integer
      startTime:
        type: string
      stopTime:
        type: string
      signedMeterValueBegin:
        type: 'null'
      signedMeterValueEnd:
        type: 'null'
      meterStart:
        type: integer
      meterStop:
        type: integer
      wattHour:
        type: integer
      duration:
        type: integer
      connectorId:
        type: integer
      socketType:
        type: string
      method:
        type: string
      amount:
        type: integer
      currency:
        type: string
      evcoId:
        type: string
      empSellerId:
        type: string
      roamingEmpId:
        type: string
      created:
        type: string
      rfid:
        type: string
      organizationId:
        type: integer
      cdrId:
        type: integer
      freeCharging:
        type: string
      UUID:
        type: string
      station:
        type: object
        properties:
          identity:
            type: string
          name:
            type: string
          location:
            type: object
            properties:
              id:
                type: integer
              address:
                type: string
              city:
                type: string
              coordinates:
                type: object
                properties:
                  lat:
                    type: number
                  lng:
                    type: number
              locationType:
                type: string
    example:
      evseId: 4482
      fullEvseId: SE*EDR*E4482
      locationId: 4421
      startTime: '1970-01-01T02:00:18+02:00'
      stopTime: '2018-04-08T18:14:01+03:00'
      signedMeterValueBegin: null
      signedMeterValueEnd: null
      meterStart: 0
      meterStop: 2863300
      wattHour: 2863300
      duration: 16777215
      connectorId: 1
      socketType: AC
      method: rfid
      amount: 0
      currency: ''
      evcoId: ''
      empSellerId: ''
      roamingEmpId: ''
      created: '2018-04-08T18:14:01+03:00'
      rfid: AABB22CCDD
      organizationId: 999
      cdrId: 281894
      freeCharging: 'no'
      UUID: 85d7427e-3b3f-11e8-8409-6c3be5a9c168
      station:
        identity: virtaglobal
        name: Virta charger
        location:
          id: 10500
          address: Energiakuja 3
          city: Helsinki
          coordinates:
            lat: 60.31139
            lng: 18.17375
          locationType: Public
  transactionReserve:
    title: Transaction - Reserve
    type: object
    properties:
      uuid:
        type: string
        format: uuid
        description: The UUID to identidy the charge
      stationUuid:
        type: string
        format: uuid
        description: The UUID to identify the station
      customerId:
        type: integer
        description: The ID to identify the customer
        minimum: 0
      amount:
        type: integer
        description: Reservation price in cents
      currency:
        type: string
        minLength: 3
        maxLength: 3
        description: Charge price's currency
      transactionType:
        type: string
        enum:
          - reserve
      created:
        type: string
        description: Time when the reservation was made
        format: date-time
    required:
      - uuid
      - stationUuid
      - customerId
      - amount
      - currency
      - transactionType
      - created
    example:
      uuid: 7d84d6dc-3b7e-47e3-9574-9d3d9e68b38b
      stationUuid: 4f33ee0c-ed46-48c9-a78f-2090e0067368
      customerId: 88679747
      amount: -50
      currency: EUR
      transactionType: reserve
      created: '2044-09-11T04:25:33.67+03:00'
  station-compact:
    title: Station - Compact
    type: object
    properties:
      identity:
        type: string
        description: Chargebox Identity of the charging station
      name:
        type: string
        description: Name of the charging station
      location:
        type: object
        items:
          type: object
          properties:
            id:
              type: integer
              description: Id of the charging station
            address:
              type: string
              description: Street and housenumber of the charging station
            city:
              type: string
              description: City of the charging station
            coordinates:
              type:
                - array
                - 'null'
              items:
                type: object
                properties:
                  lat:
                    type: number
                  lng:
                    type: number
                required:
                  - lat
                  - lng
          required:
            - id
            - address
            - city
            - coordinates
      locationType:
        type: string
        description: 'Location description of the charging station: Public, Home or Office'
    required:
      - identity
      - name
      - location
    example:
      identity: FI_CHARGE_ME
      name: Supercharger Helsinki
      location:
        id: 999
        address: Telakkakatu 2
        city: Helsinki
        coordinates:
          lat: 60.156994
          lng: 24.933734
        locationType: Public
  transactionOthers:
    title: Transaction - Others
    type: object
    properties:
      uuid:
        type: string
        format: uuid
        description: The UUID to identidy the charge
      customerId:
        type: integer
        description: The ID to identify the customer
        minimum: 0
      amount:
        type: integer
        description: Balance change amount in cents. Refund amount is always negative
      currency:
        type: string
        minLength: 3
        maxLength: 3
        description: The currency of the balance change amount
      transactionType:
        type: string
        enum:
          - refund
          - monthly
          - rfid_order
          - transition
          - seller_collection
        description: The type of transaction that changes the customers' balance.
      created:
        type: string
        format: date-time
        description: The time when the balance event was made
      customerGoupId:
        type: integer
        description: The Id the of the group the customer belongs to
      customerGroupName:
        type: string
        description: The name of the group the customer belongs to
      organizationId:
        type: integer
        description: The organization ID of the EMP of the customer
      paymentType:
        type: string
        enum:
          - Prepaid
          - Postpaid
          - Postpaid (by Card)
        description: The payment type of the customer
    required:
      - uuid
      - customerId
      - amount
      - currency
      - transactionType
      - created
      - organizationId
      - paymentType
    example:
      uuid: 898b90f7-f2c1-4677-ba77-ae2fb829c2c0
      customerId: 7050943
      amount: 37786736
      currency: EUR
      transactionType: refund
      created: '2941-04-24T22:06:40+03:00'
      customerGoupId: 6283571
      customerGroupName: mygroupname
      organizationId: 5617758
      paymentType: Postpaid
  transactionLoad:
    title: Transaction - Load
    type: object
    properties:
      uuid:
        type: string
        format: uuid
        description: The UUID to identidy the charge
      customerId:
        type: integer
        description: The ID to identify the customer
        minimum: 0
      amount:
        type: integer
        description: Balance change amount in cents. Refund amount is always negative
      currency:
        type: string
        minLength: 3
        maxLength: 3
        description: The currency of the balance change amount
      transactionType:
        type: string
        enum:
          - refund
          - monthly
          - load
          - rfid_order
          - transition
          - seller_collection
        description: The type of transaction that changes the customers' balance.
      created:
        type: string
        format: date-time
        description: The time when the balance event was made
      customerGoupId:
        type: integer
        description: The Id the of the group the customer belongs to
      customerGroupName:
        type: string
        description: The name of the group the customer belongs to
      organizationId:
        type: integer
        description: The organization ID of the EMP of the customer
      paymentType:
        type: string
        enum:
          - Prepaid
          - Postpaid
          - Postpaid (by Card)
        description: The payment type of the customer
      filingCode:
        type: integer
        description: The reference to the transaction provided by the payment gateway
    required:
      - uuid
      - customerId
      - amount
      - currency
      - transactionType
      - created
      - organizationId
      - paymentType
