  response:
    title: Customer - Response
    type: object
    properties:
      id:
        type: integer
        description: Unique identifier of the customer
      sellerId:
        type: integer
        description: Identifier of the EMP of the customer
      wantRecharge:
        type: boolean
        description: >-
          0 = no recharge, 1 = automatic recharge if customer balance < value
          defined by the EMP of the customer
      mobile:
        type: string
        description: >-
          Customers mobile number, will be used for sending SMS or logging in.
          Format is international phone number: +358401231212
        pattern: >-
          \+(9[976]\d|8[987530]\d|6[987]\d|5[90]\d|42\d|3[875]\d|
          2[98654321]\d|9[8543210]|8[6421]|6[6543210]|5[87654321]|
          4[987654310]|3[9643210]|2[70]|7|1)\d{1,14}$
      email:
        type: string
        description: Customer email address
        format: email
      language:
        type: string
        description: ISO code for language. Lowercase.
        minLength: 2
        maxLength: 2
        pattern: ^[a-z]+$
      nameFirst:
        type: string
      nameLast:
        type: string
      company:
        type: string
      wantHardware:
        type: boolean
        desription: 0 = Customer didn't order RFID, 1 = Customer ordered RFID
      freeSellers:
        type: string
        default: ''''''
        description: SellerId of CPO where the customer can charge for free
      products:
        type: string
        default: ''''''
        description: CPO ProductId that should be assigned to the customer
      address:
        type: object
        description: >-
          Only one address per customer. Note that in response address is null,
          if the object has no values.
        properties:
          street:
            type: string
          additional:
            type: string
          city:
            type: string
          country:
            type: string
          more_details:
            type: string
          zipcode:
            type: string
            description: zipcode of the address
        required:
          - street
          - city
          - country
      privateCpos:
        type: array
        description: >-
          Array of the seller Ids of private CPOs the customer should have
          access to
        default: []
        items:
          type: integer
      disabled:
        type: boolean
        default: false
        description: >-
          true = customer account is disabled, false = customer account is
          enabled
      customFieldInformation:
        type: string
        default: ''''''
        description: Free text field to store additional customer information
      billable:
        type: boolean
        default: false
        describtion: >-
          false = the customer is not billed, true = the customer is billed by
          the EMP
      wantReminder:
        type: boolean
        default: false
      wantServiceNotifications:
        type: boolean
        default: false
      wantAdvertisingNotifications:
        type: boolean
        default: false
      smsEnabled:
        type: boolean
        default: false
        description: SMS notifications are disabled by default
      brand:
        type: string
        maxLength: 50
        description: Brand the customer belongs to
      newsletter:
        type: boolean
      monthlyReceipt:
        type: boolean
      skipWelcomeEmail:
        type: boolean
      billing_type:
        type: integer
        minimum: 0
        maximum: 2
        default: 0
        description: 'Customer billing type: 0 prepaid, 1 postpaid, 2 postpaid by card'
      externalReference:
        type: string
        description: Optional reference to third party customer id
        maxLength: 64
      customerCards:
        type: array
        items:
          type: object
          properties:
            expireDate:
              type: string
            cardNumber:
              type: string
    example:
      id: 77
      sellerId: 1
      wantRecharge: false
      mobile: '+35866559923201'
      email: hello@hello.tjz
      language: en
      wantHardware: false
      nameFirst: david01
      nameLast: lallalum
      company: private co
