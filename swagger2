paths:
  /auth:
    post:
      operationId: POST_auth
      tags:
        - Authentication
      description: >-
        The Auth endpoint is used to authenticate admin users with the system to
        ensure in their identity. The Auth endpoint issues an unique expiring
        JWT token to verify the identity for future API requests.


        The token expires within 1 day. Use
        [auth/refresh](#endpoint:t8yvtPMNzHRKvn6gm) or /auth to refresh the
        token.
      parameters:
        - name: body
          in: body
          schema:
            type: object
            properties:
              username:
                type: string
              password:
                type: string
            required:
              - username
              - password
            example:
              username: user
              password: mypassword
        - name: x-api-key
          in: header
          description: Your private API key
          required: true
          type: string
        - name: Content-Type
          in: header
          required: false
          type: string
      responses:
        '200':
          description: ''
          schema:
            type: object
            properties:
              token:
                type: string
                description: >-
                  API Key (token) to use in those endpoints requiring **API
                  Key** authentication
              access_token:
                type: string
                description: >-
                  API Key (token) to use in those endpoints requiring **API
                  Key** authentication
              token_type:
                type: string
                description: Type of authentication token
              expires_in:
                type: integer
                description: Expiry time of the token in **seconds**
            required:
              - token
          examples:
            application/json:
              token: >-
                eyJ0eXAiOiJKV1EReeGciOiJIUzI1NiJ9.eyJpYXQiOjE1ODc3MjgwNzEsImp0aSI6IjZ0UTNUUXNSVkZmY2x2OUYiLCJpc3MiOiJhcGkudmlydGEtZXYuY29tIiwiZXhwIjoxNTg3ODE0NDcxLCJzdWIiOjYxMywiaHN1IjoiMmI0Y2Y0MDVjMzZkMmFiNThmNjg0ZDFjYjg4MzJmNDU5YWVmNTVkYiJ9.rA3o8-vwCUpFdrDPubv7WCJjyAGjE9qT17XlDIoC0fc
              access_token: >-
                eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9.eyJpYXQiOjE1ODc3MjgwNzEsImp0aSI6IjZ0UTNUUXNSVkZmY2x2OUYiLCJpc3MiOiJhcGkudmlydGEtZXYuY29tIiwiZXhwIjoxNTg3ODE0NDcxLCJzdWIiOjYxMywiaHN1IjoiMmI0Y2Y0MDVjMzZkMmFiNThmNjg0ZDFjYjg4MzJmNDU5YWVmNTVkYiJ9.rA3o8-vwCUpFdrDPubv7WCJjyAGjE9qT17XlDIoC0fc
              token_type: bearer
              expires_in: 86400
        '401':
          description: ''
          schema:
            type: object
            properties:
              status_code:
                type: integer
              message:
                type: string
          examples:
            application/json:
              status_code: 401
              message: >-
                Your account is locked because of too many failed login
                attempts. Please contact support@virta.fi to get your account
                opened.
        '403':
          description: ''
          schema:
            type: object
            properties:
              message:
                type: string
                description: API key is not valid
          examples:
            application/json:
              message: Forbidden
        '422':
          description: ''
          schema:
            type: object
            properties:
              status_code:
                type: integer
