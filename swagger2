  /customers/{id}/billing/refund:
    parameters:
      - name: id
        in: path
        required: true
        type: string
    post:
      operationId: POST_admin-refund
      tags:
        - Integrate external invoicing system
      description: >-
        This API can be used to refund EV drivers.


        Refunds are only allowed for customers with the billing type "prepaid"
        or "postpaid".


        Please note that all refunds will be billed to the EMP of the customer.


        Required information:


        - customer_ID

        - description

        - amount (in cents of the currency of the EMP of the EV driver) eg.
        amount 3000 = EUR 30,-
      parameters:
        - name: body
          in: body
          schema:
            type: object
            properties:
              amount:
                type: integer
                description: Amount in cents to be refunded to the EV driver
              description:
                type: string
                description: Description why the amount has been refunded to the EV driver
            required:
              - amount
              - description
      responses:
        '200':
          description: ''
          schema:
            type: object
            properties:
              uuid:
                type: string
                format: uuid
              customerId:
                type: integer
                minimum: 1
              amount:
                type: integer
                minimum: 0
              currency:
                type: string
                minLength: 3
                maxLength: 3
              transactionType:
                type: string
              created:
                type: string
                format: date-time
              customerGroupId:
                type: integer
                minimum: 1
              customerGroupName:
                type: string
              organizationId:
                type: integer
                minimum: 1
              paymentType:
                type: string
              station:
                type: array
                items: {}
          examples:
            application/json:
              uuid: badf6982-ac28-4d6f-b96e-6da5008cebcd
              customerId: 49073498
              amount: 2000
              currency: EUR
              transactionType: refund
              created: '2021-06-26T06:22:17.386Z'
              customerGroupId: 30143171
              customerGroupName: sed tempor
              organizationId: 35649368
              paymentType: Postpaid
        '403':
          description: ''
          schema:
            type: object
            properties:
              status_code:
                type: integer
              message:
                type: string
              error_code:
                type: integer
          examples:
            application/json:
              status_code: 403
              message: Cannot issue refund to postpaid by card customers
              error_code: 0
        '404':
          description: ''
