  /cpo/charges:
    get:
      operationId: GET_cpo-charges
      tags:
        - Statistics and data analysis
      description: |-
        All charges that belong to CPOs.

        Permissions needed: View CPO info or View CPO reports.
      parameters:
        - name: from
          in: query
          description: >-
            Retrieve all the charges after this timestamp. Default to UTC time 0
            (1970-01-01T00:00:00+00:00) if not defined
          type: string
          format: date-time
        - name: to
          in: query
          description: >-
            Retrieve all the charges before this timestamp. Default to current
            time if not defined
          type: string
          format: date-time
        - name: sellerId
          in: query
          description: The sellerId of the CPO
          type: integer
        - name: evseId
          in: query
          description: The EVSE id
          type: integer
        - name: rfid
          in: query
          description: The RFID
          type: string
        - name: page
          in: query
          description: >-
            If the ?page parameter is omitted, then the request will return the
            first page


            minimum: 1
          type: integer
        - name: perPage
          in: query
          description: >-
            Number of items per page. If the ?perPage is omitted, then the
            default value 25 is used. Max results count is 1000.


            Allowed Values: 0, 10, 25, 50, 100, 1000
          type: integer
        - name: Authorization
          in: header
          required: false
          type: string
      responses:
        '200':
          description: ''
          schema:
            type: object
            properties:
              evseId:
                type: integer
              fullEvseId:
                type: string
              locationId:
                type: integer
              startTime:
                type: string
              stopTime:
                type: string
              signedMeterValueBegin:
                type: string
              signedMeterValueEnd:
                type: string
              meterStart:
                type: integer
              meterStop:
                type: integer
              wattHour:
                type: integer
              duration:
                type: integer
              connectorId:
                type: integer
              socketType:
                type: string
              method:
                type: string
              amount:
                type: integer
              currency:
                type: string
              evcoId:
                type: string
              empSellerId:
                type: string
              roamingEmpId:
                type: string
              created:
                type: string
              rfid:
                type: string
              organizationId:
                type: integer
              cdrId:
                type: integer
              freeCharging:
                type: string
              UUID:
                type: string
              station:
                type: object
                properties:
                  identity:
                    type: string
                  name:
                    type: string
                  location:
                    type: object
                    properties:
                      id:
                        type: integer
                      address:
                        type: string
                      city:
                        type: string
                      coordinates:
                        type: object
                        properties:
                          lat:
                            type: number
                          lng:
                            type: number
                      locationType:
                        type: string
          examples:
            application/json:
              evseId: 1111
              fullEvseId: FI*001*E1111
              locationId: 1111
