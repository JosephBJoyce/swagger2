                      username:
                        type: array
                        items:
                          type: string
                      password:
                        type: array
                        items:
                          type: string
              error_code:
                type: integer
          examples:
            application/json:
              status_code: 422
              validation:
                errors:
                  username:
                    - The username field is required.
                  password:
                    - The password field is required.
              error_code: 0
        '429':
          description: ''
          schema:
            type: object
            properties:
              message:
                type: string
          examples:
            application/json:
              message: Limit Exceeded
