parameters:
  trait:elasticSearchPagination:limit:
    name: limit
    in: query
    description: 'Number of records. If unspecified: 100.000.'
    type: integer
  trait:elasticSearchPagination:from:
    name: from
    in: query
    description: 'Returns records from a specific point. If unspecified: 0.'
    type: integer
  trait:paginatedRequest:limit:
    name: limit
    in: query
    type: number
  trait:paginatedRequest:offset:
    name: offset
    in: query
    type: number
  trait:pagination:page:
    name: page
    in: query
    description: >-
      If the `?page` parameter is omitted, then the request will return the
      first page
    type: integer
    minimum: 1
  trait:pagination:perPage:
    name: perPage
    in: query
    description: >-
      Number of items per page. If the `?perPage` is omitted, then the default
      value 25 is used. Max results count is 100.
    type: integer
    enum:
      - 0
      - 10
      - 25
      - 50
      - 100
  trait:pagination:X-Total-Count:
    name: X-Total-Count
    in: header
    type: integer
  trait:pagination:link:
    name: link
    in: header
    type: string
    default: <URI-Reference>; rel="next"
definitions:
  connector:
    title: Connector
    type: object
    properties:
      connectorID:
        type: string
      type:
        type: string
      maxKwh:
        type: number
      maxKw:
        type: number
      currentType:
        type: string
      status:
        type: string
    required:
      - connectorID
      - type
      - maxKwh
      - maxKw
      - currentType
      - status
    example:
      connectorID: 1
      type: Mennekes
      maxKwh: 18
      maxKw: 18
      currentType: AC
      status: null
  cpoconnector:
    title: cpoConnector
    type: object
    properties:
      connectorId:
        type: integer
      type:
        type: string
        description: The plug type of the connector.
        enum:
          - Mennekes
          - Schuko
          - CHAdeMO
          - CCS
          - Type 3
          - Pantograph
          - Type 1
      power:
        type: number
        minimum: 0
        description: In kW
      currentType:
        type: string
        enum:
          - ac
          - dc
      status:
        type:
          - string
          - 'null'
        enum:
          - Operative
          - Inoperative
    example:
      connectorId: 1
      type: Type 3
      power: 22
      currentType: ac
      status: Operative
  cpoevse:
    title: cpoEvse
    type: object
    properties:
      evseId:
        type: integer
        description: The EVSE-ID of the charger
      customEvseId:
        type: string
        description: >-
          If a custom EVSE-ID is defined it is returned here. If nothing is
          defined: null
      isV2G:
        type: boolean
        description: V2G (Vehicle to Grid) charging location
      oneTimePayment:
        type: boolean
        description: Defines if Onetimepayment is enabled on the EVSE
      freeUsage:
        type: boolean
        description: Defines if the EVSE can be used for free.
      status:
        type: string
        enum:
          - available
          - busy
          - disconnected
        description: The current status of the EVSE
      adminStatus:
        type: string
        enum:
          - in-service
          - in-deployment
          - in-maintenance
          - out-of-service
        description: The admin status of the EVSE
      eichrechtType:
        type: string
        enum:
          - SAFE
          - SAM
          - Innogy
          - None
        description: The Eichrecht (German Calibration Law) type of the station.
      chargerVendor:
        type: string
        description: The vendor of the charger.
      chargerModel:
        type: string
        description: The model of the charger.
      maxPower:
        type: string
        description: The maximum power in kW of the EVSE.
      maxCurrent:
        type: integer
        description: The current limit configuration of the EVSE
      maintenanceContact:
        type:
          - string
        description: The maintenance contact information of the EVSE.
      chargeBoxIdentity:
        type: string
        description: The identity of the charger that is sent to our system.
      simNumber:
        type: string
        description: The SIM number of the charger that is sent to our system.
      iccidNumber:
        type: string
        description: The ICCID number of the charger that is sent to our system.
      softwareVersion:
        type: string
        description: The software version of the charger.
      createdDate:
        type: string
        format: date-time
        description: Timestamp when the EVSE has been created.
      lastConnectionDate:
        type: string
        format: date-time
        description: Timestamp when the EVSE has last connected to our system.
      lastUpdateDate:
        type: string
        format: date-time
        description: Timestamp when the EVSE has been last updated in our system.
      authenticationModes:
        type: array
        description: Possible authentication modes on the EVSE.
        items:
          type: string
          enum:
            - ''
            - NRC
            - NRD
            - R
            - DP
          description: >-
            NRC: Mifare classic, NRD: Mifare Desfire, R: Remote, DP: Direct
            Payment, Empty String: No authentication modes defined.
      lastIp:
        type: string
        description: Last known IPv4 or IPv6 of the charger.
      connectors:
        type: array
        items:
          $ref: '#/definitions/cpoconnector'
    example:
      id: 1235
      connectors:
        - connectorID: 1
          type: Mennekes
          maxKwh: 18
          maxKw: 18
          currentType: AC
          status: null
        - connectorID: 1
          type: Schuko
          maxKwh: 3.7
          maxKw: 3.7
          currentType: AC
          status: null
      available: true
      reservable: true
      reserved: false
      occupied: false
      isV2G: false
      currency: EUR
      pricing:
        - name: price_per_kwh
          priceCents: 0
          currency: EUR
          priceCentsWithoutVat: 0
          priceCentsVat: 0
        - name: price_per_minute
          priceCents: 3.33
          currency: EUR
          priceCentsWithoutVat: 2.77
          priceCentsVat: 0.56
        - name: price_initial_charge_fee
          priceCents: 0
          currency: EUR
          priceCentsWithoutVat: 0
          priceCentsVat: 0
        - name: price_reservation_per_minute
          priceCents: 1
          currency: EUR
          priceCentsWithoutVat: 0.83
          priceCentsVat: 0.17
      oneTimePayment: true
      oneTimePaymentInAppEnabled: true
      status: 1
      oneTimeMinimum: 50
      oneTimePricing:
        - name: price_per_kwh
          priceCents: 0
          currency: EUR
          priceCentsWithoutVat: 0
          priceCentsVat: 0
        - name: price_per_minute
          priceCents: 4.329
          currency: EUR
          priceCentsWithoutVat: 3.61
          priceCentsVat: 0.72
        - name: price_initial_charge_fee
          priceCents: 0
          currency: EUR
          priceCentsWithoutVat: 0
          priceCentsVat: 0
        - name: price_reservation_per_minute
          priceCents: 1.3
          currency: EUR
          priceCentsWithoutVat: 1.08
          priceCentsVat: 0.22
      oneTimePricingRatio: 1.3
      minutesWithoutTimeCharge: 0
      isFree: false
      remoteAuthorization: true
      evseId: FR*S37*E1235
  cpolocationpaginatedresponse:
    title: CpoLocationPaginatedResponse
    type: object
    properties:
      data:
        type: array
